package com.aug4;

public class WorkingWithCharArrays2 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("How is your day?");
		
		String theDays = sc.nextLine();
		
		switch (theDays.toLowerCase()){
		case value:
			
			break;
			
		default
			break;
		}
		
	}

}
