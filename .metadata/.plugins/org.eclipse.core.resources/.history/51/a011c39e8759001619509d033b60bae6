package fizzbuzz;

public class xmas {

	public static void main(String[]args){
		
	for (int day = 1; day < 13; day++){
		System.out.print("On the " + day);
	
		switch (day){
		case 1:
			System.out.print("st");
			break;
		case 2:
			System.out.print("nd");
			break;
		case 3:
			System.out.print("rd");
			break;
		default:
			System.out.print("th");
			break;
		}
		System.out.println(" my true love gave to me\n ");
		}
	
	
			
	
	
	
}
}
